[{"id":2933,"surah_number":26,"ayah_number":1,"text_uthmani":"بِسْمِ ٱللَّهِ ٱلرَّحْمَٰنِ ٱلرَّحِيمِ طسٓمٓ","start_time":0,"end_time":8720},{"id":2934,"surah_number":26,"ayah_number":2,"text_uthmani":"تِلْكَ ءَايَٰتُ ٱلْكِتَٰبِ ٱلْمُبِينِ","start_time":8720,"end_time":13970},{"id":2935,"surah_number":26,"ayah_number":3,"text_uthmani":"لَعَلَّكَ بَٰخِعٌۭ نَّفْسَكَ أَلَّا يَكُونُوا۟ مُؤْمِنِينَ","start_time":13970,"end_time":22070},{"id":2936,"surah_number":26,"ayah_number":4,"text_uthmani":"إِن نَّشَأْ نُنَزِّلْ عَلَيْهِم مِّنَ ٱلسَّمَآءِ ءَايَةًۭ فَظَلَّتْ أَعْنَٰقُهُمْ لَهَا خَٰضِعِينَ","start_time":22070,"end_time":37350},{"id":2937,"surah_number":26,"ayah_number":5,"text_uthmani":"وَمَا يَأْتِيهِم مِّن ذِكْرٍۢ مِّنَ ٱلرَّحْمَٰنِ مُحْدَثٍ إِلَّا كَانُوا۟ عَنْهُ مُعْرِضِينَ","start_time":37350,"end_time":51040},{"id":2938,"surah_number":26,"ayah_number":6,"text_uthmani":"فَقَدْ كَذَّبُوا۟ فَسَيَأْتِيهِمْ أَنۢبَٰٓؤُا۟ مَا كَانُوا۟ بِهِۦ يَسْتَهْزِءُونَ","start_time":51040,"end_time":63660},{"id":2939,"surah_number":26,"ayah_number":7,"text_uthmani":"أَوَلَمْ يَرَوْا۟ إِلَى ٱلْأَرْضِ كَمْ أَنۢبَتْنَا فِيهَا مِن كُلِّ زَوْجٍۢ كَرِيمٍ","start_time":63660,"end_time":75360},{"id":2940,"surah_number":26,"ayah_number":8,"text_uthmani":"إِنَّ فِى ذَٰلِكَ لَءَايَةًۭ ۖ وَمَا كَانَ أَكْثَرُهُم مُّؤْمِنِينَ","start_time":75360,"end_time":85550},{"id":2941,"surah_number":26,"ayah_number":9,"text_uthmani":"وَإِنَّ رَبَّكَ لَهُوَ ٱلْعَزِيزُ ٱلرَّحِيمُ","start_time":85550,"end_time":92660},{"id":2942,"surah_number":26,"ayah_number":10,"text_uthmani":"وَإِذْ نَادَىٰ رَبُّكَ مُوسَىٰٓ أَنِ ٱئْتِ ٱلْقَوْمَ ٱلظَّٰلِمِينَ","start_time":92660,"end_time":102010},{"id":2943,"surah_number":26,"ayah_number":11,"text_uthmani":"قَوْمَ فِرْعَوْنَ ۚ أَلَا يَتَّقُونَ","start_time":102010,"end_time":109140},{"id":2944,"surah_number":26,"ayah_number":12,"text_uthmani":"قَالَ رَبِّ إِنِّىٓ أَخَافُ أَن يُكَذِّبُونِ","start_time":109140,"end_time":118750},{"id":2945,"surah_number":26,"ayah_number":13,"text_uthmani":"وَيَضِيقُ صَدْرِى وَلَا يَنطَلِقُ لِسَانِى فَأَرْسِلْ إِلَىٰ هَٰرُونَ","start_time":118750,"end_time":128730},{"id":2946,"surah_number":26,"ayah_number":14,"text_uthmani":"وَلَهُمْ عَلَىَّ ذَنۢبٌۭ فَأَخَافُ أَن يَقْتُلُونِ","start_time":128730,"end_time":138060},{"id":2947,"surah_number":26,"ayah_number":15,"text_uthmani":"قَالَ كَلَّا ۖ فَٱذْهَبَا بِـَٔايَٰتِنَآ ۖ إِنَّا مَعَكُم مُّسْتَمِعُونَ","start_time":138060,"end_time":150650},{"id":2948,"surah_number":26,"ayah_number":16,"text_uthmani":"فَأْتِيَا فِرْعَوْنَ فَقُولَآ إِنَّا رَسُولُ رَبِّ ٱلْعَٰلَمِينَ","start_time":150650,"end_time":161540},{"id":2949,"surah_number":26,"ayah_number":17,"text_uthmani":"أَنْ أَرْسِلْ مَعَنَا بَنِىٓ إِسْرَٰٓءِيلَ","start_time":161540,"end_time":171860},{"id":2950,"surah_number":26,"ayah_number":18,"text_uthmani":"قَالَ أَلَمْ نُرَبِّكَ فِينَا وَلِيدًۭا وَلَبِثْتَ فِينَا مِنْ عُمُرِكَ سِنِينَ","start_time":171860,"end_time":182150},{"id":2951,"surah_number":26,"ayah_number":19,"text_uthmani":"وَفَعَلْتَ فَعْلَتَكَ ٱلَّتِى فَعَلْتَ وَأَنتَ مِنَ ٱلْكَٰفِرِينَ","start_time":182150,"end_time":191210},{"id":2952,"surah_number":26,"ayah_number":20,"text_uthmani":"قَالَ فَعَلْتُهَآ إِذًۭا وَأَنَا۠ مِنَ ٱلضَّآلِّينَ","start_time":191210,"end_time":203720},{"id":2953,"surah_number":26,"ayah_number":21,"text_uthmani":"فَفَرَرْتُ مِنكُمْ لَمَّا خِفْتُكُمْ فَوَهَبَ لِى رَبِّى حُكْمًۭا وَجَعَلَنِى مِنَ ٱلْمُرْسَلِينَ","start_time":203720,"end_time":217090},{"id":2954,"surah_number":26,"ayah_number":22,"text_uthmani":"وَتِلْكَ نِعْمَةٌۭ تَمُنُّهَا عَلَىَّ أَنْ عَبَّدتَّ بَنِىٓ إِسْرَٰٓءِيلَ","start_time":217090,"end_time":231460},{"id":2955,"surah_number":26,"ayah_number":23,"text_uthmani":"قَالَ فِرْعَوْنُ وَمَا رَبُّ ٱلْعَٰلَمِينَ","start_time":231460,"end_time":237490},{"id":2956,"surah_number":26,"ayah_number":24,"text_uthmani":"قَالَ رَبُّ ٱلسَّمَٰوَٰتِ وَٱلْأَرْضِ وَمَا بَيْنَهُمَآ ۖ إِن كُنتُم مُّوقِنِينَ","start_time":237490,"end_time":251100},{"id":2957,"surah_num