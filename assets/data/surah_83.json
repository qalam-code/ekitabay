[{"id":5849,"surah_number":83,"ayah_number":1,"text_uthmani":"بِسْمِ ٱللَّهِ ٱلرَّحْمَٰنِ ٱلرَّحِيمِ وَيْلٌۭ لِّلْمُطَفِّفِينَ","audio_url":"https://cdn.islamic.network/quran/audio/128/ar.alafasy/5849.mp3","page":587},{"id":5850,"surah_number":83,"ayah_number":2,"text_uthmani":"ٱلَّذِينَ إِذَا ٱكْتَالُوا۟ عَلَى ٱلنَّاسِ يَسْتَوْفُونَ","audio_url":"https://cdn.islamic.network/quran/audio/128/ar.alafasy/5850.mp3","page":587},{"id":5851,"surah_number":83,"ayah_number":3,"text_uthmani":"وَإِذَا كَالُوهُمْ أَو وَّزَنُوهُمْ يُخْسِرُونَ","audio_url":"https://cdn.islamic.network/quran/audio/128/ar.alafasy/5851.mp3","page":587},{"id":5852,"surah_number":83,"ayah_number":4,"text_uthmani":"أَلَا يَظُنُّ أُو۟لَٰٓئِكَ أَنَّهُم مَّبْعُوثُونَ","audio_url":"https://cdn.islamic.network/quran/audio/128/ar.alafasy/5852.mp3","page":587},{"id":5853,"surah_number":83,"ayah_number":5,"text_uthmani":"لِيَوْمٍ عَظِيمٍۢ","audio_url":"https://cdn.islamic.network/quran/audio/128/ar.alafasy/5853.mp3","page":587},{"id":5854,"surah_number":83,"ayah_number":6,"text_uthmani":"يَوْمَ يَقُومُ ٱلنَّاسُ لِرَبِّ ٱلْعَٰلَمِينَ","audio_url":"https://cdn.islamic.network/quran/audio/128/ar.alafasy/5854.mp3","page":587},{"id":5855,"surah_number":83,"ayah_number":7,"text_uthmani":"كَلَّآ إِنَّ كِتَٰبَ ٱلْفُجَّارِ لَفِى سِجِّينٍۢ","audio_url":"https://cdn.islamic.network/quran/audio/128/ar.alafasy/5855.mp3","page":588},{"id":5856,"surah_number":83,"ayah_number":8,"text_uthmani":"وَمَآ أَدْرَىٰكَ مَا سِجِّينٌۭ","audio_url":"https://cdn.islamic.network/quran/audio/128/ar.alafasy/5856.mp3","page":588},{"id":5857,"surah_number":83,"ayah_number":9,"text_uthmani":"كِتَٰبٌۭ مَّرْقُومٌۭ","audio_url":"https://cdn.islamic.network/quran/audio/128/ar.alafasy/5857.mp3","page":588},{"id":5858,"surah_number":83,"ayah_number":10,"text_uthmani":"وَيْلٌۭ يَوْمَئِذٍۢ لِّلْمُكَذِّبِينَ","audio_url":"https://cdn.islamic.network/quran/audio/128/ar.alafasy/5858.mp3","page":588},{"id":5859,"surah_number":83,"ayah_number":11,"text_u